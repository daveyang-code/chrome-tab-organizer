{
  "name": "Tab Organizer",
  "description": "Sort and group tabs",
  "version": "1.0",
  "manifest_version": 3,
  "permissions": ["tabs", "tabGroups", "scripting"],
  "action": {
    "default_popup": "popup.html"
  },
  "host_permissions": ["*://www.youtube.com/watch?v=*"],
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}
